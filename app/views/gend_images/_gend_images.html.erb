<div class="thumb-index well">

  <div class="btn-toolbar">

    <div class="btn-group">
      <button class="btn select-all">Select All</button>
      <button class="btn select-none disabled disable-none-selected enable-some-selected">Select None</button>
    </div>

    <div class="btn-group">
      <button class="btn disabled disable-none-selected enable-some-selected">
        Delete <span class="selected-count badge badge-info">0</span>
      </button>
    </div>

  </div>

  <ul class="thumbnails">
    <% @gend_images.each do |gi| %>
        <li>
          <div class="thumbnail">
            <%= render :partial => gi %>
          </div>
        </li>
    <% end %>
  </ul>

  <%= link_to 'More images', :gend_images %>

</div>
