<% unless current_user %>
    <p>
      <%= link_to "Login", new_session_path %> to upload source images and create source sets.
    </p>
<% end %>

<% unless @src_sets.empty? %>
    <%= render :partial => 'src_sets/src_sets' %>
<% end %>

<% if @src_images %>
    <%= render :partial => 'src_images/src_images' %>
<% end %>

<% if @gend_images %>
    <%= render :partial => 'gend_images/gend_images' %>
<% end %>
